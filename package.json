{
  "name": "equine-army-js-bot",
  "version": "1.0.0",
  "description": "A discord bot for equine army discord server",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon build/index.js\"",
    "deploy:commands": "tsc && node build/deploy-commands.js",
    "lint": "eslint --ext .ts,.tsx src --report-unused-disable-directives",
    "lint-fix": "npm run lint -- --fix"
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">= 16.6.0"
  },
  "dependencies": {
    "@discordjs/builders": "0.12.0",
    "@discordjs/rest": "0.3.0",
    "discord-api-types": "0.29.0",
    "discord.js": "13.6.0",
    "dotenv": "16.0.0",
    "emoji-regex": "10.0.1",
    "express": "4.17.3",
    "firebase": "9.6.9",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.180",
    "@typescript-eslint/parser": "5.15.0",
    "concurrently": "7.0.0",
    "eslint": "8.11.0",
    "nodemon": "2.0.15",
    "typescript": "4.6.2"
  }
}
